{{#each models ~}}
{{#if isObject ~}}
export interface {{ name }} {
    {{#each fields}}
    {{ name }}: {{#if isArray}}Array<{{/if}}{{ type }}{{#if isArray}}>{{/if}}{{#unless isRequired}} | null{{/unless}};
    {{/each ~}}
}

{{/if ~}}

{{#if isEnum ~}}
export type {{ name }} = {{#each enumValues }}"{{ value }}"{{#unless @last}} | {{/unless}}{{/each ~}};

{{/if ~}}
{{/each ~}}

{{#each documents ~}}
export namespace {{ name }} {
  {{#if hasVariables}}
      export interface Variables {
      {{#each variables}}
          {{ name }}: {{#if isArray}}Array<{{/if}}{{ type }}{{#if isArray}}>{{/if}}{{#unless isRequired}} | null{{/unless}};
      {{/each}}
      }

  {{/if ~}}

  {{#each innerTypes}}
      {{#if usingFragments}}
      export type {{ name }} = {{#each fragmentsUsed ~}}{{this}} & {{/each }}{
      {{#each fields}}
        {{ name }}: {{#if isArray}}Array<{{/if}}{{ type }}{{#if isArray}}>{{/if}};
      {{/each}}
      }
      {{else}}
      export interface {{name}} {
        {{#each fields}}
          {{ name }}: {{#if isArray}}Array<{{/if}}{{ type }}{{#if isArray}}>{{/if}};
        {{/each}}
      }
      {{/if}}

    {{/each ~}}

{{#each resultFields}}
      export interface {{ name }}Result {
      {{#each fields}}
      {{ name }}: {{#if isArray}}Array<{{/if}}{{ type }}{{#if isArray}}>{{/if}};
      {{/each}}
      }

  {{/each ~}}
}

{{/each ~}}
