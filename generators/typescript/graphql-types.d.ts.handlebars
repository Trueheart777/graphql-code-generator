{{#each models ~}}
    {{#if isObject ~}}
    export interface {{ name }} {
        {{#each fields}}
        {{ name }}: {{#if isArray}}Array<{{/if}}{{ type }}{{#if isArray}}>{{/if}}{{#unless isRequired}} | null{{/unless}}{{#unless @last}},{{/unless}}
        {{/each ~}}
    };

    {{/if ~}}

    {{#if isEnum ~}}
    export enum {{ name }} {
        {{#each enumValues }}
        {{ name }} = <any>"{{ value }}"{{#unless @last}},{{/unless}}
        {{/each ~}}
    };

    {{/if ~}}
{{/each ~}}

{{#each documents ~}}
    {{#if hasVariables ~}}
    export interface {{ name }}Variables {
        {{#each variables}}
        {{ name }}: {{#if isArray}}Array<{{/if}}{{ type }}{{#if isArray}}>{{/if}}{{#unless isRequired}} | null{{/unless}}{{#unless @last}},{{/unless}}
        {{/each ~}}
    };

    {{/if ~}}

    export namespace {{ name }} {
      {{#each innerTypes}}
          export interface {{name}} {
            {{#each fields}}
            {{ name }}: {{#if isArray}}Array<{{/if}}{{ type }}{{#if isArray}}>{{/if}}{{#unless @last}},{{/unless}}
            {{/each}}
          };

        {{/each ~}}

    {{#each resultFields}}
          export interface {{ name }}Result {
          {{#each fields}}
          {{ name }}: {{#if isArray}}Array<{{/if}}{{ type }}{{#if isArray}}>{{/if}}{{#unless @last}},{{/unless}}
          {{/each}}
          };

      {{/each ~}}
    };

{{/each ~}}
