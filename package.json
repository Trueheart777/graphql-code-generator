{
  "name": "graphql-codegen",
  "version": "0.0.1",
  "description": "GraphQL types and code generator based on schema",
  "main": "dist/index.js",
  "scripts": {
    "watch": "tsc --watch",
    "clean": "rimraf ./dist",
    "lint": "tslint src/**/*.ts",
    "prebuild": "npm run clean && npm run lint",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./dist/index.js",
    "debug": "node --inspect --debug-brk ./dist/index.js",
    "dev": "nodemon --ext ts,js,d.ts,template,graphql,json,handlebars ./dist/index.js",
    "test": ""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotansimha/graphql-codegen.git"
  },
  "bin": {
    "graphql-codegen": "dist/index.js"
  },
  "keywords": [
    "graphql",
    "codegen",
    "generator",
    "apollo",
    "node",
    "typescript",
    "ts",
    "flow",
    "types",
    "d.ts",
    "typings"
  ],
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dotansimha/graphql-codegen/issues"
  },
  "homepage": "https://github.com/dotansimha/graphql-codegen#readme",
  "devDependencies": {
    "cz-conventional-changelog": "^1.2.0",
    "rimraf": "^2.5.4",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@types/commander": "^2.3.31",
    "@types/glob": "^5.0.30",
    "@types/graphql": "^0.8.2",
    "@types/handlebars": "^4.0.31",
    "@types/node": "^0.0.2",
    "@types/pascal-case": "^0.0.29",
    "@types/request": "^0.0.36",
    "commander": "^2.9.0",
    "glob": "^7.1.1",
    "graphql": "^0.8.2",
    "handlebars": "^4.0.6",
    "nodemon": "^1.11.0",
    "pascal-case": "^2.0.0",
    "request": "^2.79.0"
  }
}
