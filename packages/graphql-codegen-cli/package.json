{
  "name": "graphql-code-generator",
  "version": "0.8.15",
  "license": "MIT",
  "bin": {
    "gql-gen": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dotansimha/graphql-codegen.git"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "tslint src/**/*.ts",
    "prebuild": "yarn clean && yarn lint",
    "build": "tsc",
    "prestart": "yarn build",
    "start": "cd dist && node index.js",
    "debug": "cd dist && node --inspect --debug-brk index.js",
    "test": "jest --no-cache --verbose --runInBand"
  },
  "keywords": [
    "gql",
    "generator",
    "code",
    "types",
    "interfaces",
    "graphql",
    "codegen",
    "apollo",
    "node",
    "typescript",
    "ts",
    "flow",
    "types",
    "d.ts",
    "typings"
  ],
  "author": "Dotan Simha <dotansimha@gmail.com>",
  "bugs": {
    "url": "https://github.com/dotansimha/graphql-codegen/issues"
  },
  "homepage": "https://github.com/dotansimha/graphql-codegen#readme",
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "./tests/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      ".*?handlebars-helpers.*?",
      "src/index.ts",
      "src/types.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/tests/jest.typescript.js"
    },
    "testRegex": "tests/.*\\.ts$",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/glob": "5.0.35",
    "@types/jest": "22.2.0",
    "@types/mkdirp": "0.5.2",
    "@types/node": "9.4.7",
    "@types/request": "2.47.0",
    "graphql": "0.13.1",
    "graphql-tag": "2.8.0",
    "jest": "22.4.2",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || || ^0.13.0"
  },
  "dependencies": {
    "commander": "2.15.0",
    "glob": "7.1.2",
    "graphql-codegen-compiler": "0.8.15",
    "graphql-codegen-core": "0.8.15",
    "graphql-codegen-generators": "0.8.15",
    "mkdirp": "0.5.1",
    "request": "2.85.0"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  }
}
