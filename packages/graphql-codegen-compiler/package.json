{
  "name": "graphql-codegen-compiler",
  "version": "0.8.19",
  "description": "GraphQL code generator compiler",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prebuild": "rimraf ./dist/"
  },
  "devDependencies": {
    "@types/common-tags": "1.4.0",
    "@types/jest": "22.2.0",
    "@types/node": "9.4.7",
    "graphql": "0.13.1",
    "graphql-tag": "2.8.0",
    "graphql-tools": "2.22.0",
    "jest": "22.4.2",
    "rimraf": "2.6.2",
    "ts-jest": "22.4.1",
    "tslint": "5.9.1",
    "typescript": "2.7.2"
  },
  "dependencies": {
    "@types/handlebars": "4.0.36",
    "change-case": "3.0.2",
    "common-tags": "1.7.2",
    "graphql-codegen-core": "0.8.19",
    "graphql-codegen-generators": "0.8.19",
    "handlebars": "4.0.11",
    "moment": "2.21.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/tests/jest.typescript.js"
    },
    "testRegex": "tests/.*\\.ts$",
    "testEnvironment": "node"
  },
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "main": "./dist/index.js"
}
