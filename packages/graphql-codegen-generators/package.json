{
  "name": "graphql-codegen-generators",
  "version": "0.6.0-alpha.cc47c396",
  "description": "GraphQL code generator templates",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "pretest": "yarn build",
    "build": "webpack",
    "bundledts": "dts-generator --out dist/index.d.ts --project ./ --name graphql-codegen-generators --main 'graphql-codegen-generators/src/index'",
    "postbundledts": "rimraf ./dist/src/",
    "postbuild": "npm run bundledts"
  },
  "devDependencies": {
    "@types/common-tags": "^1.2.5",
    "@types/graphql": "^0.9.3",
    "@types/jest": "^20.0.2",
    "@types/node": "7",
    "awesome-typescript-loader": "^3.2.1",
    "common-tags": "^1.4.0",
    "graphql": "^0.10.3",
    "graphql-codegen-core": "0.6.0-alpha.a6fea330",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.0.0",
    "jest": "^20.0.4",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "tslint": "^5.4.3",
    "typescript": "^2.4.1",
    "webpack": "^3.0.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@types/change-case": "^2.3.1",
    "@types/handlebars": "^4.0.33",
    "change-case": "^3.0.1",
    "dts-generator": "^2.1.0",
    "handlebars": "^4.0.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/tests/jest.typescript.js"
    },
    "testRegex": "tests/.*\\.ts$",
    "testEnvironment": "node"
  }
}
