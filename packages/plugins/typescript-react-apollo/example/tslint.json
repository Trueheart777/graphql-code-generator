{
  "extends": ["tslint-react"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "tools/**/*.js", "node_modules/**/*.ts"]
  },
  "rules": {
    "import-sources-order": false,
    "named-imports-order": false,
    "import-ordering": false,
    "no-string-literal": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false
  },
  "defaultSeverity": "warning"
}
