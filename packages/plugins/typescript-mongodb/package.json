  },
  "devDependencies": {
    "@graphql-codegen/testing": "0.18.0",
    "jest": "24.5.0",
    "ts-jest": "24.0.0",
    "typescript": "3.3.3333",
    "flow-bin": "0.95.1",
    "flow-parser": "0.95.1",
    "graphql": "14.1.1"
