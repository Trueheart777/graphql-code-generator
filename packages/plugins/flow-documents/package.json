{
  "name": "graphql-codegen-flow-documents",
  "version": "0.14.5",
  "description": "GraphQL Code Generator plugin for generating Flow types for GraphQL docuemnts (query/mutation/subscription/fragment)",
  "repository": "git@github.com:dotansimha/graphql-code-generator.git",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "codegen-templates-scripts build",
    "pretest": "yarn build",
    "test": "codegen-templates-scripts test"
  },
  "dependencies": {
    "graphql-codegen-flow": "0.14.5",
    "graphql-codegen-core": "0.14.5",
    "graphql-codegen-plugin-helpers": "0.14.5"
  },
  "devDependencies": {
    "flow-parser": "0.87.0",
    "flow-bin": "0.88.0",
    "codegen-templates-scripts": "0.14.5",
    "graphql": "14.0.2"
  },
  "main": "./dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": false
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
